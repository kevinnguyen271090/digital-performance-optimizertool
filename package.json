{
  "name": "digital-performance-optimizer",
  "version": "1.0.0",
  "description": "Enterprise-grade digital marketing analytics platform",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && uvicorn app.main:app --reload",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "setup": "npm run install:all && npm run setup:db",
    "setup:db": "echo 'Run SQL scripts in scripts/ folder'",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && uvicorn app.main:app",
    "test": "cd frontend && npm test",
    "lint": "cd frontend && npm run lint",
    "format": "cd frontend && npm run format"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "marketing",
    "analytics",
    "dashboard",
    "react",
    "fastapi",
    "supabase",
    "typescript",
    "python"
  ],
  "author": "Kevin Nguyen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/digital-performance-optimizer.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7"
  }
}
